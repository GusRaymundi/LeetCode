/**
 * Note: The returned array must be malloced, assume caller calls free().
 */
char** fizzBuzz(int n, int* returnSize) {
    char** s = malloc(sizeof(char*) * n);
    if (s == NULL) {
        *returnSize = 0;
        return NULL;
    }
    *returnSize = n;
    for (int i = 1; i <= n; i++) {
        if (i % 3 == 0 && i % 5 == 0) {
            s[i-1]=malloc(9*sizeof(char));
            s[i-1] = "FizzBuzz";
        } else if (i % 3 == 0) {
            s[i-1]=malloc(5*sizeof(char));
            s[i-1] = "Fizz";
            
        } else if (i % 5 == 0) {
            s[i-1]=malloc(5*sizeof(char));
            s[i-1] = "Buzz";
        } else {
            s[i-1]=malloc(12*sizeof(char));
            
             sprintf(s[i-1], "%d", i);
        }
    }
    return s;
}
